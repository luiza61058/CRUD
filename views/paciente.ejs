<%- include('layout') -%>
<br><br><br>
<div class="container">
  <table class="table table-striped table-hover table-responsive">
    <tr>
      <th>CPF</th>
      <th>Nome Completo</th>
      <th>Idade</th>
      <th>Data da Marcação</th>
      <th>Hora da Marcação</th>
    </tr>
    <% pacientes.forEach(paciente => { %>
    <tr>
      <td><%= paciente.cpf %></td>
      <td><%= paciente.nome_completo %></td>
      <td><%= paciente.idade %></td>
      <td><%= paciente.dia_marc %></td>
      <td><%= paciente.hora_marc %></td>
    </tr>
    <% }); %>
  </table>
</div>
